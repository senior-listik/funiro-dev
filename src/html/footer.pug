- const menu = [{ title: "Menu", list: ["Products", "Rooms", "Inspirations", "About Us", "Terms & Policy"] }, { title: "Account", list: ["My Account", "Checkout", "My Cart", "My catalog"] }, { title: "Stay Connected", list: ["Facebook", "Instagram", "Twitter"]}];

footer.footer
    .footer__container 
        .footer__body 
            .footer__main 
                a.footer__logo._footer-title(href="") Funiro.
                .footer__text 
                    | Worldwide furniture store since 2020. 
                    | We sell over 1000+ branded products on our website
                .footer__contacts.contacts-footer
                    a.contacts-footer__item._icon-location(href="")
                        | Sawojajar Malang, Indonesia
                    a.contacts-footer__item._icon-phone(href="tel:+62894563455")
                        | +6289 456 3455
                    a.contacts-footer__item(href="http://www.funiro.com", target="_blank") www.funiro.com
            .footer__menu.menu-footer(data-spollers="768,max")
                each item in menu
                    .menu-footer__column
                        button.menu-footer__title._footer-title(type="button" data-spoller)= item.title
                        ul.menu-footer__list
                            each listItem in item.list
                                li
                                    a.menu-footer__link(href="")= listItem 
            .footer__subscribe.subscribe
                .subscribe__title._footer-title Stay Updated
                form.subscribe__form(action="#", data-message="subscribe", data-test)
                    input.subscribe__input._req._email._input(type="text", autocpmplete="off", name="form[]", data-error="Ошибка", data-value="Enter your email")
                    button.subscribe__button._icon-send(type='submit')

